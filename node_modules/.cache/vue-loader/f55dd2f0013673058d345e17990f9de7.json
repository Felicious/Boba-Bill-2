tFriendId = 1;\r\nexport default {\r\n  components: {\r\n    InputText,\r\n    Friend\r\n  },\r\n  data() {\r\n    return {\r\n      newFriendName: \"\",\r\n      friends: [\r\n        {\r\n          id: nextFriendId++,\r\n          text: \"Derrick\"\r\n        },\r\n        {\r\n          id: nextFriendId++,\r\n          text: \"Bunbun\"\r\n        }\r\n      ],\r\n      selectedFriends: []\r\n    };\r\n  },\r\n  methods: {\r\n    addFriend() {\r\n      // TODO: what is this trimmed text for LOL just copying rn\r\n      const trimmedText = this.newFriendName.trim();\r\n      if (trimmedText) {\r\n        this.friends.push({\r\n          id: nextFriendId++,\r\n          text: trimmedText\r\n        });\r\n        this.newFriendName = \"\"; // clear\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}